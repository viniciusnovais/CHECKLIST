        </activity>

        <activity
            android:name="br.com.pdasolucoes.checklist.activities.AppointmentActivity">
        </activity>

        <activity
            android:name="br.com.pdasolucoes.checklist.activities.QuestionsActivity">
        </activity>

        <activity
            android:name="br.com.pdasolucoes.checklist.activities.QueryActivity">
        </activity>

        <activity
            android:name="br.com.pdasolucoes.checklist.activities.ForgetPassActivity">
        </activity>

        <activity android:name="br.com.pdasolucoes.checklist.activities.FrameTodoActivity"></activity>

        <activity android:name="br.com.pdasolucoes.checklist.activities.ListaQuery"></activity>

        <activity android:name="br.com.pdasolucoes.checklist.activities.ListaRespostaFaltaTodo"></activity>

