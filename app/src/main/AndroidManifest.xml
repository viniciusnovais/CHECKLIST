            android:name="br.com.pdasolucoes.checklist.activities.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:label=""
            android:name="br.com.pdasolucoes.checklist.activities.HomeActivity">
        </activity>

        <activity
        android:name="br.com.pdasolucoes.checklist.activities.StartActivity">
    </activity>

        <activity
